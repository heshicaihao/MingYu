<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="MenuDrawer" />

    <style name="MenuDrawer.Widget" />

    <style name="MenuDrawer.Widget.Title">
        <item name="android:textAppearance">?android:attr/textAppearance</item>
        <item name="android:textSize">18sp</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">32dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:drawablePadding">16dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="TitleTextViewStyle">
        <item name="android:textSize">15sp</item>
    </style>

    <style name="AboutMYStyle">
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:padding">10dp</item>
        <item name="android:background">#ffffff</item>
    </style>

    <style name="ContentTextViewStyle">
        <item name="android:textSize">15dp</item>
        <item name="android:typeface">normal</item>
    </style>

    <style name="RedTextViewStyle">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#ec4849</item>
    </style>

    <style name="ContentStyle">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginTop">80dp</item>
    </style>

    <style name="RegisterStyle">
        <item name="android:layout_marginLeft">12dp</item>
        <item name="android:layout_marginRight">12dp</item>
    </style>

    <style name="MingYuStyle">
        <item name="android:layout_marginLeft">12dp</item>
        <item name="android:layout_marginRight">12dp</item>
    </style>

    <style name="MingYuGridViewStyle">
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
    </style>

    <style name="ContentEditTextStyle">
        <item name="android:background">@null</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="SpinnerEditTextStyle">
        <item name="android:background">@null</item>
        <item name="android:textColor">@color/textDark</item>
        <item name="android:textSize">8sp</item>
    </style>

    <style name="RedButtonStyle">
        <item name="android:textColor">#ffffff</item>
        <item name="android:background">#ec4849</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="goldenButtonStyle">
        <item name="android:textColor">#ffffff</item>
        <item name="android:background">@color/golden</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="MiddleButtonStyle">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@drawable/middle_red_button</item>
    </style>

    <style name="BigRedButtonStyle">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">14sp</item>
        <item name="android:padding">10dp</item>
        <item name="android:background">#ec4849</item>
    </style>

    <style name="BigRedButtonStyleSuccess">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">14sp</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="RightArrowGoldenTextStyle">
        <item name="android:textColor">#c8a238</item>
        <item name="android:drawableRight">@drawable/golden_arrow</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <!-- 新版增加  begin -->
    <style name="NEW_TitleTextViewStyle">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="dark_TitleTextViewStyle">
        <item name="android:textSize">15sp</item>
    </style>

    <style name="pinkBG">
        <item name="android:background">#efecec</item>
    </style>
    <!-- 新版增加  end -->

    <style name="Translucent_NoTitle" parent="android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">#00000000</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <!-- 日历样式 -->
    <style name="main_bar_text_style">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="Theme.Loading" parent="android:Theme">
        <item name="android:windowBackground">@drawable/startpage</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>

</resources>